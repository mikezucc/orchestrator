# HTTPS Configuration (optional)
# Set to true to enable HTTPS for the Vite dev server
VITE_HTTPS_ENABLED=false

# SSL Certificate paths (relative to frontend root)
VITE_SSL_CERT_PATH=./certs/cert.pem
VITE_SSL_KEY_PATH=./certs/key.pem

# Backend HTTPS Configuration
# Set to true if your backend is running on HTTPS
VITE_BACKEND_HTTPS=false