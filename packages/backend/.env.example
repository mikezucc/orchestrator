DATABASE_URL=postgres://orchestrator:orchestrator@localhost:5432/orchestrator
PORT=3000
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:3000/api/auth/google/callback
JWT_SECRET=your-jwt-secret-key-change-this-in-production

# GitHub OAuth Configuration
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
GITHUB_REDIRECT_URI=http://localhost:3000/api/github-auth/callback

# Frontend URL (for OAuth redirects)
FRONTEND_URL=http://localhost:5173
BACKEND_URL=http://localhost:3000

# CORS Configuration
# Comma-separated list of allowed origins
# Examples: http://localhost:5173,https://localhost:5173,http://192.168.1.100:5173,http://slopbox.dev,https://slopbox.dev,https://api.slopbox.dev
# For slopbox.dev deployment: http://slopbox.dev,https://slopbox.dev,http://www.slopbox.dev,https://www.slopbox.dev
CORS_ORIGINS=http://localhost:5173,https://localhost:5173,http://localhost:3000,https://localhost:3000,http://localhost,https://localhost

# HTTPS Configuration (optional)
HTTPS_ENABLED=false
SSL_CERT_PATH=./certs/cert.pem
SSL_KEY_PATH=./certs/key.pem

# SendGrid Configuration
SENDGRID_API_KEY=your-sendgrid-api-key
SENDGRID_FROM_EMAIL=noreply@slopbox.dev
SENDGRID_FROM_NAME=Slopbox